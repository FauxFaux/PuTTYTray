210 DIALOG DISCARDABLE 0, 0, 140, 74
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Pageant: Enter Passphrase"
FONT 8, "MS Shell Dlg"
BEGIN
    CTEXT "Enter passphrase for key", 100, 10, 6, 120, 8
    CTEXT "", 101, 10, 16, 120, 8
    EDITTEXT 102, 10, 26, 120, 12, ES_PASSWORD | ES_AUTOHSCROLL
    CONTROL "&Save this passphrase",103,"Button",BS_AUTOCHECKBOX | WS_TABSTOP, 10, 42, 120, 10
    DEFPUSHBUTTON "O&K", IDOK, 20, 56, 40, 14
    PUSHBUTTON "&Cancel", IDCANCEL, 80, 56, 40, 14
END

