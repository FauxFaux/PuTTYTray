Set makefile Makefile.mpw

#	Run Make, then execute its output.

Echo "# `Date -t` ----- Analyzing dependencies."
Begin
	Echo "Set Echo 1"
	Make {"Parameters"} -f "{makefile}"
End > make.out
Echo "# `Date -t` ----- Executing build commands."
make.out
Delete make.out
Echo "# `Date -t` ----- Done."
